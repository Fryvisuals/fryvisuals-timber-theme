{% block html_head_container %}

    {% include 'html-header.twig' %}
    {% block head %}
    {% endblock %}
    </head>
{% endblock %}
<script>console.log({{ _context | json_encode | raw }});</script>
<body class="{{ body_class }}">
<a class="skip-link screen-reader-text" href="#content">{{ _e( 'Skip to content') }}</a>
<header class="header">
    {% block header %}
        <div class="header__wrapper">
            <h1 class="header-logo" role="banner" style="width: 30rem;">
                <a class="header-logo-link" href="{{ site.url }}" rel="home" title="{{ site.name }}"><img loading="lazy" src="{{ custom_logo_url }}" class="header-logo-image" /></a>
            </h1>
            <nav id="nav-main" class="nav-main" role="navigation">
                {% include "menu.twig" with {'items': primary_menu.get_items} %}
            </nav><!-- #nav -->
        </div>
    {% endblock %}
</header>

<section id="content" role="main" class="content-wrapper">
    {% if title %}<h1>{{ title }}</h1>{% endif %}
    <div class="wrapper {{ sidebar_class }}">
        {% block content %}
            Sorry, no content
        {% endblock %}
    </div>
    {% if sidebar %}
        <aside class="layout-sidebar">
            {{ sidebar }}
        </aside>
    {% endif %}
</section>

{% block footer %}
    <footer id="footer">
        {% include 'footer.twig' %}
    </footer>
    {{ function('wp_footer') }}
{% endblock %}
</body>
</html>
