{% block html_head_container %}

    {% include 'html-header.twig' %}
    {% block head %}
    {% endblock %}
    </head>
{% endblock %}
<script>console.log({{ _context | json_encode | raw }});</script>
<body class="{{ body_class }}">
<a class="skip-link screen-reader-text" href="#content">{{ _e( 'Skip to content') }}</a>
<header class="header">
    {% block header %}
        <div class="header__wrapper">
            <div class="header-logo" role="banner">
                <a class="header-logo-link" href="{{ site.url }}" rel="home"><img loading="lazy" src="{{ custom_logo_url }}" class="header-logo-image" /></a>
            </div>
            <button class="hamburger" type="button" data-toggle="nav-menu">
                <span></span>
                <span></span>
                <span></span>
            </button>
            <nav id="nav-main" class="{{ primary_menu.slug }}" role="navigation">
                {% include "menu.twig" with {'items': menu.get_items} %}
            </nav><!-- #nav -->
        </div>

    {% endblock %}
</header>

<section id="content" role="main" class="content-wrapper">
    {% if sidebar %}<div class="wrapper {{ sidebar_class }}">{% endif %}
        {% block content %}
            Sorry, no content
        {% endblock %}
    {% if sidebar %}</div>{% endif %}
    {% if sidebar %}
        <aside class="layout-sidebar">
            {{ sidebar }}
        </aside>
    {% endif %}
</section>

{% block footer %}
    <footer id="footer">
        {% include 'footer.twig' %}
    </footer>
    {{ function('wp_footer') }}
{% endblock %}
</body>
</html>
